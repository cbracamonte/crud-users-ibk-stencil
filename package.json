{
  "name": "crud-user-ibk-stencil",
  "version": "1.0.0",
  "author": "Hernan Bracamonte",
  "scripts": {
    "build": "stencil build --prerender",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "generate": "stencil generate",
    "docker-build": "docker build --no-cache --progress=plain -t microfrontend-crud-user-ibk .",
    "docker-run": "docker run -d -it -p 3333:3333/tcp microfrontend-crud-user-ibk",
    "docker-stop": "docker container stop microfrontend-crud-user-ibk",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^3.2.2",
    "@stencil/router": "^1.0.1",
    "@storybook/addon-a11y": "^7.3.2",
    "@storybook/addon-actions": "^7.3.2",
    "@storybook/addon-backgrounds": "^7.3.2",
    "@storybook/addon-console": "^2.0.0",
    "@storybook/addon-docs": "^7.3.2",
    "@storybook/addon-jest": "^7.3.2",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-viewport": "^7.3.2",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/web-components": "^7.3.2",
    "broadcast-channel-hb": "^1.1.1",
    "html2canvas": "^1.4.1",
    "marked": "^5.0.2",
    "storybook-readme": "^5.0.9"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@storybook/addon-essentials": "^7.3.2",
    "@storybook/addon-interactions": "^7.3.2",
    "@storybook/addon-links": "^7.3.2",
    "@storybook/blocks": "^7.3.2",
    "@storybook/html": "^7.3.2",
    "@storybook/html-webpack5": "^7.3.2",
    "@storybook/testing-library": "^0.2.0",
    "@types/jest": "^29.5.1",
    "@types/puppeteer": "^5.4.7",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "lit-html": "^2.8.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "puppeteer": "^20.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.3.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-stencil": "^1.0.1",
    "workbox-build": "^6.5.4"
  }
}
